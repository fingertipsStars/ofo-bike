/* pages/blings/index.wxss */

.container{
  width:100%;
  height:100vh;
  display:flex;
  flex-direction:column;
  align-items:center;
  justify-content:space-between;
}

.number,.endride{
  flex:2;
  width:100%;
  text-align:center;
  padding:60rpx 0;
}

.time{
  flex:6;
  width:100%;
  text-align:center;
}

.time .time-content{
  width:100%;
  font-size:160rpx;
}

.endride button{
  width:90%;
  margin-top:10px;
}