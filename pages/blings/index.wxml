<!--pages/blings/index.wxml-->
<view class="container">

  <view class="number">

    单车编号：{{number}}
  </view>
  <view class="time">
    <view class="time-title">{{actionText}}</view>
    <view class="time-content">{{hours}}:{{minutes}}:{{seconds}}</view>

  </view>

  <view class="endride">
    <button type="warn" bindtap="endride" disabled="{{clickBtn}}">结束骑行</button>
    <button type="primary" bindtap ="backToMap">回到地图</button>
  </view>
</view>